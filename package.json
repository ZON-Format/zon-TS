    "jest": "^30.2.0",
    "js-yaml": "^4.1.1",
    "llama-tokenizer-js": "^1.2.2",
    "msgpack-lite": "^0.1.26",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@azure/openai": "^2.0.0",
    "dotenv": "^17.2.3",
    "openai": "^6.9.1",
    "tson": "^0.0.10"
  }
