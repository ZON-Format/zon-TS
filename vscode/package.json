{
  "name": "zon-format",
  "displayName": "ZON Format",
  "description": "Syntax highlighting for ZON (Z-Object Notation) files",
  "version": "1.0.0",
  "publisher": "zon-format",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "zon",
        "aliases": ["ZON", "zon"],
        "extensions": [".zon", ".zonf"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "zon",
        "scopeName": "source.zon",
        "path": "./syntaxes/zon.tmLanguage.json"
      }
    ]
  }
}
